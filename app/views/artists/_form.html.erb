<h1><%= @artist.id ? "Edit artist" : "Create artist" %></h1>
<%= form_for @artist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true, autofocus: true %>
  <%= f.label :songs %>
  <%= f.text_field :songs %>
  <%= f.label :avavter %>
  <%= f.text_field :avatar %>
  <%= f.submit %>
  <% end %>